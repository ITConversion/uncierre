            <div class="col-md-12">
                <h1 class="text-white my-3">Leads generados</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-4">
                <p class="text-white">
                    Proyecto / Landing
                </p>
                <select id="landing-select" class="form-select" aria-label="Default select example">
                    <option>Selecione</option>
                </select>
            </div>
            <div class="col-4">
                <p class="text-white">
                    Fecha:
                </p>
                <input id="input-date" type="date" class="form-control"/>
            </div>
        </div>
    </div>
</div>
<div class="container bg-white py-4">
    <div class="row">
        <div class="col-md-12">
            <table class="table" id="leads-table">
                <thead>
                <tr class="border">
                    <th>Fecha</th>
                    <th>Campos</th>
                    <th>UTM</th>
                </tr>
                </thead>
                <tbody>

                </tbody>
            </table>
        </div>
        <div class="col-12">
            <div id="app"></div>
        </div>
    </div>
</div>
